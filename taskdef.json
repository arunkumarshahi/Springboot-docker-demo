{
  "family": "spring-boot-hello",

  "containerDefinitions": [
    {
      "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
      "name": "spring-boot-hello",
      "cpu": 10,
      "memory": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
           "hostPort": 8080
        }
      ]
    }
  ]
}
